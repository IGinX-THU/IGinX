[
  {
    "group": "iginx",
    "name": "iginx",
    "className": "org.apache.zeppelin.iginx.IginxInterpreter",
    "properties": {
      "iginx.host": {
        "envName": null,
        "propertyName": "iginx.host",
        "defaultValue": "127.0.0.1",
        "description": "IginX server host to connect to, Default = 127.0.0.1",
        "type": "string"
      },
      "iginx.port": {
        "envName": null,
        "propertyName": "iginx.port",
        "defaultValue": "6888",
        "description": "IginX server port to connect to, Default = 6888",
        "type": "number"
      },
      "iginx.username": {
        "envName": null,
        "propertyName": "iginx.username",
        "defaultValue": "root",
        "description": "Username for authentication, Default = 'root'",
        "type": "string"
      },
      "iginx.password": {
        "envName": null,
        "propertyName": "iginx.password",
        "defaultValue": "root",
        "description": "Password for authentication, Default = 'root'",
        "type": "password"
      },
      "iginx.time.precision": {
        "envName": null,
        "propertyName": "iginx.time.precision",
        "defaultValue": "ms",
        "description": "the time precision to display. Default = 'ms'",
        "type": "string"
      },
      "iginx.outfile.dir": {
        "envName": null,
        "propertyName": "iginx.outfile.dir",
        "defaultValue": "/your/outfile/path",
        "description": "physical path to storage download files, change it to your own path",
        "type": "string"
      },
      "iginx.fetch.size": {
        "envName": null,
        "propertyName": "iginx.fetch.size",
        "defaultValue": "1000",
        "description": "Number of files downloaded at one batch, Default = '1000'",
        "type": "string"
      },
      "iginx.outfile.max.num": {
        "envName": null,
        "propertyName": "iginx.outfile.max.num",
        "defaultValue": "100",
        "description": "The max number of dir in outfile path, Default = '100'",
        "type": "string"
      },
      "iginx.outfile.max.size": {
        "envName": null,
        "propertyName": "iginx.outfile.max.size",
        "defaultValue": "10240",
        "description": "The max size(MB) of dir in outfile path, Default = '10240'",
        "type": "number"
      }
    },
    "editor": {
      "language": "sql",
      "editOnDblClick": false,
      "completionKey": "TAB"
    }
  }
]
